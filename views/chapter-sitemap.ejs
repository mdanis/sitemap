<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>Extramarks</title>
        <link href="<%= siteUrl %>/public/css/extramark.css" type="text/css" rel="stylesheet" />
    </head>

    <body>

        <div id="bannerContainer" class="bannerContainer_sitemap">
            <div class="aboutusContainer" style="position:relative;">

                <!--sitemap class-->
                <ul class="sitmapClass">
                   <% for(var i=0; i< classes.length; i++) { %>
                    <li><a href="/sitemap/<%= boardName %>/<%= classes[i].name %>/<%= boardContainerId %>/<%= classes[i].rack_id %>">
                    <span class="shortName"><%= classes[i].name %></span>class - <%= classes[i].name %>
                    </a>
                    </li>
                   <% } %>
                </ul>
                <!--sitemap class end-->


                <div style="border-bottom:1px solid #ddd; padding-bottom:10px; margin-bottom:10px;">
                    <h1 class="crisp-splash-header"> Class- <%= selectedClass %> </h1>
                    <a href="javascript:;" style="float:right"><img src="<?php echo $this->basePath() ?>/images/back.png"></a>
                    <div class="clear">&nbsp;</div>
                </div>

                <div class="crisp-g">
                   <% for(var i=0; i< data.length; i++) { %>
                    <% if(data[i].chapters.length > 0) { %>
                    <div class="crisp-u-1-3">
                        <h2 class="skill-tree-skills-header">
                           <%= data[i].name %>
                        </h2>
                        <ul class="skill-tree-skills">
                            <% for(var j=0; j< data[i].chapters.length;j++){ %>
                                <li class="skill-tree-skill-node">
         <a href="<%= redirectSiteUrl %>/website/index/dashboard/<%= rack_id %>#/chapterDetails/<%= data[i].chapters[j].rack_id %>" target="_blank">
	                                <span class="skill-tree-skill-name">
	                                	<%= data[i].chapters[j].name %>
	                                </span>
	                                </a>
                                </li>
                            <% } %>
                             
                        </ul>
                    </div>
                    <% }; %>
                    <% } %>
                </div>



                <p style="font-size:11px;"><strong>Note</strong>: All Contents are dynamic</p>
                <div class="clear">&nbsp;</div>
            </div>
        </div>

    </body>
</html>
