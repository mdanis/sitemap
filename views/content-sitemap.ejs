<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Extramarks</title>
<link href="<%= siteUrl %>/public/css/extramark.css" type="text/css" rel="stylesheet" />
</head>

<body>

<div id="bannerContainer" class="bannerContainer_sitemap">
  <div class="aboutusContainer">
<div style="border-bottom:1px solid #ddd; padding-bottom:10px; margin-bottom:10px;">
	<h1 class="crisp-splash-header"><%= boardName %></h1>
        <div class="clear">&nbsp;</div>
    </div>
     
 <div class="crisp-g">
                   <% for(var i=0; i< data.length; i++) { %>
                    <div class="crisp-u-1-3">
                        <h2 class="skill-tree-skills-header">
                            <a href="/sitemap/<%= boardName %>/<%= data[i].name %>/<%= boardContainerId %>/<%= data[i].rack_id %>" ><%= data[i].name %></a>
                        </h2>
                        <ul class="skill-tree-skills">
                            <% for(var j=0; j< data[i].subjects.length;j++){ %>
                                <li class="skill-tree-skill-node">
	                                <span class="skill-tree-skill-name">
	                                	<%= data[i].subjects[j].name %>
	                                </span>
                                </li>
                            <% } %>
                        </ul>
                    </div>
                    <% }; %>
                </div>
      
      
      
     <p style="font-size:11px;"><strong>Note</strong>: All Contents are dynamic</p>
    <div class="clear">&nbsp;</div>
  </div>
</div>
<script type="text/javascript" src="/socket.io/socket.io.js"></script>
<script type="text/javascript" src="<%= siteUrl %>/public/js/custom.js"></script>
</body>
</html>
